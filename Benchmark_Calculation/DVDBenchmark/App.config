<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
	</startup>
	<runtime>
		<gcServer enabled="true"/>
	</runtime>
	<appSettings>
		<!--local-->
		<add key="EndPointUrl" value="https://localhost:8081/"/>
		<add key="AuthorizationKey" value=""/>
		<add key="DatabaseName" value="datavalidator-dev"/>
		<add key="CollectionName" value="deli-telemetry"/>
		
		<!--dev-->
		<!--<add key="EndPointUrl" value=""/>
		<add key="AuthorizationKey" value=""/>
		<add key="DatabaseName" value="sample"/>
		<add key="CollectionName" value="deli-telemetry"/>-->

		<add key="CollectionThroughput" value="4000"/>
		<!--Default consistency is session and through put is 400 , maxx throughput is 4000-->
		<add key="ShouldCleanupOnStart" value="true"/>
		<add key="ShouldCleanupOnFinish" value="false"/>

		<!-- determined by the tool -->
		<add key="DegreeOfParallelism" value="-1"/>
		<!-- 1440 - telemetry records generated by 1 site 1 device -->
		<!-- 7200 - telemetry records generated by 1 site 5 devices -->
		<add key="NumberOfDocumentsToInsert" value="7200"/> 

		<!--<add key="CollectionPartitionKey" value="/deviceId_date"/>
		<add key="Device1" value="Fryer.json"/>
		<add key="Device2" value="Oven.json"/>
		<add key="Device3" value="Rotisserie.json"/>
		<add key="Device4" value="TypeA.json"/>
		<add key="Device5" value="TypeB.json"/>-->
	</appSettings>
	<system.diagnostics>
		<trace autoflush="false" indentsize="4">
			<listeners>
				<add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
			</listeners>
		</trace>
	</system.diagnostics>
</configuration>
